{
  "name": "vite-react-antd",
  "version": "0.0.1",
  "description": "Vite + Typescript + React + Antd template",
  "repository": {
    "type": "git",
    "url": "git@github.com:zyj1022/vite-react-antd.git"
  },
  "author": {
    "name": "zhiyanjie",
    "email": "zyj10222@126.com",
    "github": "https://github.com/zyj1022"
  },
  "scripts": {
    "prepare": "husky install",
    "start": "npm run mock | npm run dev",
    "dev": "vite --mode development",
    "build": "vite build --mode release",
    "build:beta": "vite build --mode beta",
    "build:legacy": "vite build --mode legacy ",
    "preview": "vite preview",
    "prebuild": "rimraf dist",
    "serve": "vite preview",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx --no-error-on-unmatched-pattern --quiet --fix ./src",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "mock": "nodemon ./mock/app.js"
  },
  "keywords": [
    "vite",
    "typescript",
    "react",
    "mobx",
    "antd",
    "eslint",
    "prettier"
  ],
  "license": "MIT",
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@types/react": "^17.0.27",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-config": "^5.0.3",
    "@types/react-router-dom": "^5.3.1",
    "antd": "^4.16.13",
    "axios": "^0.22.0",
    "dayjs": "^1.10.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^5.2.1",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-angular": "^13.2.0",
    "@commitlint/config-conventional": "^13.2.0",
    "@types/pino": "^6.3.11",
    "@types/node": "^16.10.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@vitejs/plugin-legacy": "^1.6.1",
    "@vitejs/plugin-react": "^1.0.2",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "autoprefixer": "^10.3.7",
    "conventional-changelog-cli": "^2.1.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "fastify": "^3.22.0",
    "fs": "^0.0.1-security",
    "history": "^5.0.1",
    "husky": "^7.0.2",
    "less": "^4.1.2",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^11.2.3",
    "mobx": "^6.3.3",
    "mobx-react": "^7.2.0",
    "mobx-react-lite": "^3.2.1",
    "mobx-react-router": "^4.1.0",
    "nodemon": "^2.0.13",
    "omit.js": "^2.0.2",
    "path": "^0.12.7",
    "postcss": "^8.3.9",
    "pre-commit": "^1.2.2",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "rollup-plugin-visualizer": "^5.5.2",
    "styled-components": "^5.3.1",
    "typescript": "^4.4.3",
    "vite": "^2.6.5",
    "vite-aliases": "^0.8.7",
    "vite-plugin-babel-import": "^2.0.5",
    "vite-plugin-compression": "^0.3.5",
    "vite-plugin-imp": "^2.0.9",
    "vite-plugin-style-import": "^1.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,tsx,ts,json}": [
      "npm run lint",
      "git add --force"
    ],
    "**/*": "prettier --write ."
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
